	<!--RULE -->
<xsl:template match="/cda:ClinicalDocument/cda:component/cda:structuredBody/cda:component/cda:section/cda:component/cda:section/cda:entry/cda:procedure[cda:code/@code='XX-MCH219']/cda:approachSiteCode"
